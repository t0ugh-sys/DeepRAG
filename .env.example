# Large model settings (OpenAI-compatible)
OPENAI_API_KEY=sk-your-api-key
OPENAI_BASE_URL=https://api.deepseek.com/v1
RAG_MODEL=deepseek-chat

# Available models (comma-separated)
AVAILABLE_MODELS=deepseek-chat,qwen-turbo,qwen-plus,qwen-max

# Qwen (optional)
QWEN_API_KEY=sk-your-qwen-key
QWEN_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# Vector store
VECTOR_BACKEND=auto
MILVUS_HOST=127.0.0.1
MILVUS_PORT=19530

# Retrieval settings
RAG_TOP_K=8
RAG_BM25_ENABLED=true
RAG_BM25_WEIGHT=0.4
RAG_VEC_WEIGHT=0.6
RAG_MMR_LAMBDA=0.7
RAG_RERANKER_ENABLED=false

# Web search (optional)
RAG_WEB_SEARCH_ENABLED=false
SERPER_API_KEY=

# Multi-tenant / namespace
RAG_NAMESPACE=default
RAG_NAMESPACE_WHITELIST=default
RAG_API_KEY_NAMESPACE=default
RAG_API_KEY=
RAG_API_KEY_REQUIRED=false

# CORS
RAG_CORS_ALLOW_ORIGINS=http://localhost:5173,http://127.0.0.1:5173
RAG_CORS_ALLOW_CREDENTIALS=true
RAG_CORS_ALLOW_METHODS=*
RAG_CORS_ALLOW_HEADERS=*
